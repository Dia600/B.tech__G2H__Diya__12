#include <stdio.h>
int main() {
char ch ;
     printf("Enter the alphabet : ");
     scanf(" %c",&ch);
     ch = tolower(ch);
     if(ch >='a' && ch <= 'z'){
         if(ch == 'a'|| ch=='e'||ch=='i'||ch=='o'||ch=='u')
            printf("It is a vowel");
         else 
         printf("It is a consonant");}
    else { 
        printf("Not alphabet.");
return 0 ; }

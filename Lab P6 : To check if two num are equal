#include <stdio.h>
#include <ctype.h>
#include <math.h>
int main() {
    int a,b ;
    printf("Enter num 1-");
    scanf("%d",&a);
    printf("Enter num 2-");
    scanf("%d",&b);
    if (a==b){
        printf("Both the numbers are equal.");}
    else{ 
        printf("Both the numbers are not equal.");} 
    return 0;
}
